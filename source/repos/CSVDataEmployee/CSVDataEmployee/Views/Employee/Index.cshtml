@{
}
@model List<CSVDataEmployee.Models.Employee>

<h2>Employee List</h2>

<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Position</th>
        <th>Date of Birth</th>
        <th>Email</th>
        <th>Salary</th>
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.Name</td>
            <td>@employee.Position</td>
            <td>@employee.DateOfBirth.ToShortDateString()</td>
            <td>@employee.Email</td>
            <td>@employee.Salary</td>
        </tr>
    }
</table>
